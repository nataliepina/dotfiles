[user]
	name = nataliepina
	email = natalie.a.pina@gmail.com
	signingkey = <INSERT_KEY>

[core]
excludesfile = ~/.gitignore

[commit]
gpgSign = true

[rebase]
autoStash = true

[pull]
rebase = true

[push]
default = current

[init]
defaultBranch = main

[gpg]
program = gpg

[url "git@github.com:"]
insteadOf = https://github.com/

[diff]
renameLimit = 16384
