[core]
excludesfile = ~/.gitignore

[commit]
gpgsign = true

[rebase]
autoStash = true

[pull]
rebase = true

[init]
defaultBranch = main

[gpg]
program = gpg2

[url "git@github.com:"]
insteadOf = https://github.com/

[diff]
renameLimit = 16384
